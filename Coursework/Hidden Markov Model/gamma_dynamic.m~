function [gamma] = gamma_dynamic(alpha, beta) 
    length_v = length(alpha);
    length_P = length(P);
    gamma =[0 0 0; 0 0 0];
    for t = 1:3
        for i=1:2
            %gamma(i,t)=(alpha(i,t)*beta(i,t))/((alpha(1,t)*beta(1,t)+(alpha(2,t)*beta(2,t))));
            sum(t) = sum(t) + (alpha(j,1)*beta(j,1));
        end
        gamma(i,1) = alpha(i,1)*beta(i,1)/sum_1;
    end
end


 